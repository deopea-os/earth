    "lint": "TIMING=1 eslint --cache --cache-location \".turbo/.eslintcache\""
  },
  "devDependencies": {
    "@deopea/eslint-config": "^1.0.0",
    "@deopea/tsconfig": "^1.0.0",
    "typescript": "*"
