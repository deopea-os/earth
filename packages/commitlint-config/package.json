{
  "name": "@deopea/commitlint-config",
  "version": "1.0.0",
  "description": "commitlint config used by the residents of deopea",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
