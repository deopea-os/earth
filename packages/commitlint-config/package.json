    "@commitlint/config-conventional": "^17.4.2",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deopea-os/earth.git",
    "directory": "packages/commitlint-config"
  },
  "keywords": [
    "commitlint",
    "commitlintconfig"
  ],
  "author": {
    "name": "David",
    "url": "https://github.com/deopea-david"
  },
