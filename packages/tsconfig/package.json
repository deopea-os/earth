{
  "name": "@deopea.os/tsconfig",
  "version": "2.0.0",
  "private": false,
  "description": "tsconfigs used by residents of deopea",
  "type": "module",
  "main": "lib/base.json",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/base.json",
    "./browser": "./lib/browser.json",
    "./cjs": "./lib/cjs.json",
    "./node": "./lib/node.json",
    "./qwik": "./lib/qwik.json"
  },
  "scripts": {
    "fmt": "prettier --write --cache --cache-location \".turbo/.prettier-cache\" .",
    "lint": "TIMING=1 eslint --cache --cache-location \".turbo/.eslintcache\"",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deopea-os/earth.git",
    "directory": "packages/tsconfig"
  },
  "keywords": [
    "tsconfig"
  ],
  "author": {
    "name": "David",
    "url": "https://github.com/deopea-david"
  },
  "license": "MIT",
  "homepage": "https://github.com/deopea-os/earth/tree/main/packages/tsconfig",
  "bugs": {
    "url": "https://github.com/deopea-os/earth/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tsconfig/create-react-app": "^2.0.2",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/recommended": "^1.0.3",
    "@tsconfig/strictest": "^2.0.3",
    "@tsconfig/vite-react": "^3.0.1"
  }
}
