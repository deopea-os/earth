{
  "name": "@deopea.os/eslint-config",
  "version": "2.0.0",
  "private": false,
  "description": "eslint configs used by residents of deopea",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./base": "./lib/base.js",
    "./browser": "./lib/browser.js",
    "./cjs": "./lib/cjs.js",
    "./node": "./lib/node.js",
    "./qwik": "./lib/qwik.js"
  },
  "scripts": {
    "build": "tsc",
    "fmt": "prettier --write --cache --cache-location \".turbo/.prettier-cache\" .",
    "lint": "TIMING=1 eslint --cache --cache-location \".turbo/.eslintcache\"",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qwik": "^1.5.1",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.1.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.5",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "typescript": "^5.0.0"
  },
  "homepage": "https://github.com/deopea-os/earth/tree/main/packages/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/deopea-os/earth.git",
    "directory": "packages/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": {
    "name": "David",
    "url": "https://github.com/deopea-david"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deopea-os/earth/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^8.57.0",
    "globals": "^14.0.0",
    "read-package-up": "^11.0.0"
  }
}
