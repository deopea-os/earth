{
  "name": "@deopea.os/eslint-config",
  "version": "2.0.0-alpha.1",
  "private": false,
  "description": "eslint configs used by residents of deopea",
  "type": "commonjs",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "fmt": "prettier --write --cache --cache-location \".turbo/.prettier-cache\" .",
    "lint": "TIMING=1 eslint --cache --cache-location \".turbo/.eslintcache\"",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-qwik": "^1.2.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.5",
    "typescript": "*"
  },
  "homepage": "https://github.com/deopea-os/earth/tree/main/packages/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/deopea-os/earth.git",
    "directory": "packages/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": {
    "name": "David",
    "url": "https://github.com/deopea-david"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deopea-os/earth/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
