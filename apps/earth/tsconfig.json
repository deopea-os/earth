{
  "extends": "@deopea.os/tsconfig/browser",
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "dist",

    "incremental": true,
    "isolatedModules": true,
    "noEmit": true,

    "types": ["vite/client"],

    "paths": {
      "components/*": ["components/*"],
      "routes/*": ["routes/*"],
      "utils/*": ["utils/*"]
    }
  },
  "files": ["./vite.config.ts"],
  "include": ["src"]
}
